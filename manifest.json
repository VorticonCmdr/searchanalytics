{
  "manifest_version": 2,
  "name": "Search Analytics extension",
  "short_name": "Search Analytics",
  "description": "This extension shows the queries for the current page (if available). The data source is the Google Search Analytics API.",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "identity",
    "https://content.googleapis.com/"
  ],
  "content_security_policy": "script-src 'self' https://apis.google.com/; object-src 'self'",
  "oauth2": {
    "client_id": "410928017212-ci45iriirpfipf8rugbd7qvc94t13qto.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/webmasters.readonly"
    ]
  }
}
