{
  "manifest_version": 3,
  "name": "Search Analytics alternative",
  "short_name": "SAa",
  "version": "1.0",
  "description": "A Chrome extension to interact with the Google Search Console API.",
  "key": "",
  "permissions": ["identity", "storage", "unlimitedStorage"],
  "host_permissions": ["https://www.googleapis.com/"],
  "background": {
    "service_worker": "/js/background.js"
  },
  "action": {
    "default_title": "GSC alternative UI"
  },
  "icons": {
    "180": "/icons/logo-180.png",
    "192": "/icons/logo-192.png"
  },
  "oauth2": {
    "client_id": "410928017212-ci45iriirpfipf8rugbd7qvc94t13qto.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/webmasters.readonly"]
  }
}
